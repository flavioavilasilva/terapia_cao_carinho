<%= form_for @therapist_dog do |f| %>
  <%= f.label :name, "Nome" %>
  <%= f.text_field :name %>
  <%= f.label :date_of_birth, "Data de nascimento" %>
  <%= f.text_field :date_of_birth %>
  <%= f.label :breed, "Raça" %>
  <%= f.text_field :breed %>
  <%= f.label :status, "Status" %>
  <%= f.text_field :status %>
  <%= f.label :description, "Descrição" %>
  <%= f.text_area :description %>
  <%= f.submit "Cadastrar Cão terapeuta" %>
<% end %>
